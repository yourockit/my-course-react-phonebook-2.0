{
  "name": "goal-craft",
  "homepage": "https://github.com/Alexandrbig1/goal-craft",
  "private": true,
  "version": "0.0.0",
  "author": "Alex Smagin",
  "description": "GoalCraft - Simplify goal setting and tracking with this React and TypeScript project. Empower yourself to articulate your goals, save them securely in local storage, and navigate your journey towards success.",
  "keywords": [
    "typescript",
    "react",
    "todo",
    "goals",
    "bootcamp",
    "fullstack"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.5",
    "@reduxjs/toolkit": "^2.3.0",
    "@types/styled-components": "^5.1.32",
    "axios": "^1.7.7",
    "gh-pages": "^6.1.1",
    "modern-normalize": "^2.0.0",
    "nanoid": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.12.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/node": "^20.10.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
